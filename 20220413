#include <iostream>
using namespace std;

int main(){
/*
jika nilai >= 90 Lulus sangat memusaskan
jika nilai >= 80 Lulus memusaskan
jika nilai >= 75 Cukup
tidak boleh > 100
tidak boleh < 0
< 75 tidak lulus
*/
int nilai;
cout<<"Masukan Nilai = "; cin>>nilai;
if (nilai > 100) {
    cout<<"Di luar jangkauan";
}
else if (nilai >= 90 ) {
    cout<<"lulus sangat memuaskan";
}

else if (nilai >= 80 ) {
    cout<<"lulus memuaskan";
}
else if (nilai >= 75) {
    cout<<"cukup";
}
else if (nilai <0){
    cout<<"Di luar jangkauan";
}
else {
    cout<<"tidak lulus";
}
    return 0;
}
